{
  "**/*.{html,xml,ts,css,scss,env,dockerignore,sh}": [
    "license-check-and-add add -f licenses/config.json -r 2021",
    "vue-cli-service lint"
  ],
  "**/*.{js,vue}": [
    "license-check-and-add add -f licenses/config.json -r 2021",
