{
  "name": "business-partner-agent-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "test:unit": "vue-cli-service test:unit",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint && npm run license-file-headers-add",
    "license-file-headers-add": "license-check-and-add add -f licenses/config.json -r 2021",
    "license-check": "license-checker --production --json --onlyAllow 'MIT;Python-2.0;Apache-2.0;BSD-2-Clause;BSD-3-Clause' --excludePackages 'business-partner-agent-frontend@0.1.0'",
    "license": "generate-attribution -o ./licenses",
    "i18n:report": "vue-cli-service i18n:report --src './src/**/*.?(js|vue)' --locales './src/locales/**/*.json'",
    "prepare": "cd ../ && husky install ./frontend/.husky"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "core-js": "^3.6.4",
    "csv-string": "^4.0.1",
    "deepmerge": "^4.2.2",
    "font-awesome": "^4.7.0",
    "material-design-icons-iconfont": "^6.1.0",
    "moment": "^2.29.1",
    "qrcode.vue": "^1.7.0",
    "vue": "2.6.14",
    "vue-advanced-chat": "^1.1.0",
    "vue-i18n": "^8.24.5",
    "vue-json-pretty": "^1.7.0",
    "vue-markdown-render": "^1.0.2",
    "vue-moment": "^4.1.0",
    "vue-native-websocket": "^2.0.14",
    "vue-router": "^3.4.9",
    "vuetify": "2.6.1",
    "vuex": "^3.6.0"
  },
  "devDependencies": {
    "@babel/core": "7.16.0",
    "@babel/eslint-parser": "7.16.3",
    "@fortawesome/fontawesome-free": "5.15.4",
    "@mdi/font": "6.5.95",
    "@mdi/js": "6.5.95",
    "@vue/cli-plugin-babel": "4.5.15",
    "@vue/cli-plugin-eslint": "4.5.15",
    "@vue/cli-plugin-router": "4.5.15",
    "@vue/cli-plugin-unit-jest": "4.5.15",
    "@vue/cli-service": "4.5.15",
    "@vue/eslint-config-prettier": "6.0.0",
    "@vue/test-utils": "1.3.0",
    "eslint": "7.32.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-unicorn": "39.0.0",
    "eslint-plugin-vue": "8.1.1",
    "husky": "7.0.4",
    "license-check-and-add": "4.0.3",
    "license-checker": "25.0.1",
    "lint-staged": "12.1.2",
    "oss-attribution-generator": "^1.7.1",
    "prettier": "2.5.0",
    "sass": "1.44.0",
    "sass-loader": "10.2.0",
    "vue-cli-plugin-i18n": "2.3.1",
    "vue-cli-plugin-vuetify": "2.4.4",
    "vue-jest": "3.0.7",
    "vue-template-compiler": "2.6.14",
    "vuetify-loader": "1.7.3"
  }
}
